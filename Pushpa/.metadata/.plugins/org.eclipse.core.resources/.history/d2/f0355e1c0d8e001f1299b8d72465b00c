	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a[] = new int[10];;
		if(a.length<10) {
  System.out.println("Enter no:");
  int no=sc.nextInt();
    hash(a,no);
		}else {
			System.out.println("Hash table is full!!");
		}
  
	}

	

}
