<%-include('menu')%>
<div class="container">
  <h1>show user</h1>
  <%=data%>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">Handle</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(value){%>
      <tr>
        <th scope="row">1</th>
        <td><%=value.name%></td>
        <td><%=value.email%></td>
        <td><%=value.mobile%></td>
      </tr>
      <%})%>
    </tbody>
  </table>
</div>
